#
# Modified from French keyboard layout, AZERTY style.
# Build Apple style layout
#

type OVERLAY

map key 16 A
map key 17 Z
map key 30 Q
map key 39 M
map key 44 W
map key 50 COMMA
map key 51 SEMICOLON
map key 41 PLUS
map key 86 GRAVE

### ROW 1

key GRAVE {
    label:                              '@'
    base:                               '@'
    shift:                              '#'
    alt:                                '\u2022'
    shift+alt:                          '\u0178'
}

key 1 {
    label:                              '1'
    base:                               '&'
    shift:                              '1'
    alt:                                '\uf8ff'
    shift+alt:                          '\u00b4'
}

key 2 {
    label:                              '2'
    base:                               '\u00e9'
    shift:                              '2'
    alt:                                '\u00eb'
    shift+alt:                          '\u201e'
}

key 3 {
    label:                              '3'
    base:                               '"'
    shift:                              '3'
    alt:                                '\u201c'
    shift+alt:                          '\u201d'
}

key 4 {
    label:                              '4'
    base:                               '\''
    shift:                              '4'
    alt:                                '\u2018'
    shift+alt:                          '\u2019'
}

key 5 {
    label:                              '5'
    base:                               '('
    shift:                              '5'
