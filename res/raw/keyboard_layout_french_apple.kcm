}

key 9 {
    label:                              '9'
    base:                               '\u00e7'
    shift:                              '9'
    alt:                                '\u00c7'
    shift+alt:                          '\u00c1'
}

key 0 {
    label:                              '0'
    base:                               '\u00e0'
    shift:                              '0'
    alt:                                '\u00f8'
    shift+alt:                          '\u00d8'
